<resources>
    <attr name="titleText" format="string" />
    <attr name="subtitleText" format="string" />
    <attr name="subtitleHint" format="string" />

    <declare-styleable name="TextInputView">
        <attr name="titleText" />
        <attr name="editTextHint" format="string" />
    </declare-styleable>

    <declare-styleable name="SelectPhotosView">
        <attr name="maxPhotos" format="integer" />
    </declare-styleable>

    <declare-styleable name="PropertyDetailView">
        <attr name="propertyName" format="string" />
    </declare-styleable>

    <declare-styleable name="SelectionSpinnerView">
        <attr name="titleText" />
    </declare-styleable>

    <declare-styleable name="TitleSubTitleView">
        <attr name="titleText" />
        <attr name="subtitleText" />
        <attr name="subtitleHint" />
        <attr name="titleStyle" format="reference" />
        <attr name="subtitleStyle" format="reference" />
    </declare-styleable>
</resources>